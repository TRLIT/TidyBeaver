{
    "App": {
      "Env": "development",
      "Port": "8080",
      "LogLevel": "debug",
      "LogAmount": "50"
    },
    "Database": {
      "Host": "localhost",
      "Port": "5432",
      "User": "postgres",
      "Password": "yourpassword",
      "Name": "logdb",
      "SSLMode": "disable"
    },
    "API": {
      "BaseURL": "https://api.example.com",
      "AuthToken": "your_api_token",
      "TimeoutSeconds": "30"
    },
    "LogPaths": {
      "LocalLogFolder": "C:/logs",
      "IncludeSubDirs": true
    },
    "Microservices": {
      "AuthServiceURL": "http://localhost:8001",
      "PaymentServiceURL": "http://localhost:8002",
      "LogServiceURL": "http://localhost:8003"
    },
    "WindowsEventLog": {
      "Enabled": true,
      "Channels": [
        "Application",
        "System",
        "Security"
      ],
      "Query": "*[System/Level=2]"
    }
  }
  